        {
            "role": "participant_1",
            "content": {
                "emotion_scores": {},
                "text": "Thanks! I try."
            }
        },
        {
            "role": "participant_2",
            "content": {
                "emotion_scores": {},
                "text": "What do you like to do for fun?"
            }
        },
        {
            "role": "participant_1",
            "content": {
                "emotion_scores": {},
                "text": "I like to read, write, and go to the movies."
