        {
            "role": "participant_1",
            "content": {
                "emotion_scores": {},
                "text": "What do you do for a living?"
            }
        },
        {
            "role": "participant_2",
            "content": {
                "emotion_scores": {},
                "text": "I'm a veterinarian."
            }
        },
        {
            "role": "participant_1",
            "content": {
                "emotion_scores": {},
                "text": "Oh, that's cool. I've always wanted to be a veterinarian."
