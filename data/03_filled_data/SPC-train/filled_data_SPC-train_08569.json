    "messages": [
        {
            "role": "participant_1",
            "content": {
                "emotion_scores": {},
                "text": "Hi! I'm a big fan of the Grateful Dead. Have you ever seen them in concert?"
            }
        },
        {
            "role": "participant_2",
            "content": {
                "emotion_scores": {},
                "text": "Yes, I used to follow them around when they toured. I saw them in concert over 100 times."
