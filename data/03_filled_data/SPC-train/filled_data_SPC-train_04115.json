        {
            "role": "participant_2",
            "content": {
                "emotion_scores": {},
                "text": "I like to draw."
            }
        },
        {
            "role": "participant_1",
            "content": {
                "emotion_scores": {},
                "text": "That's cool! What kind of things do you like to draw?"
