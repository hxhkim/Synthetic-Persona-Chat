        {
            "role": "participant_2",
            "content": {
                "emotion_scores": {},
                "text": "You're welcome!"
            }
        },
        {
            "role": "participant_1",
            "content": {
                "emotion_scores": {},
                "text": "So, what kind of music do you like to listen to?"
