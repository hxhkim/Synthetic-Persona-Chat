            "content": {
                "emotion_scores": {
                    "Anger": 0,
                    "Fear": 0,
                    "Joy": 25,
                    "Sadness": 0,
                    "Surprise": 0,
                    "Love": 35,
                    "Boredom": 0,
                    "Neutral": 40
