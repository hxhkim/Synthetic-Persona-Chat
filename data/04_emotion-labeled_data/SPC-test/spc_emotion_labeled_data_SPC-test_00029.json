                "emotion_scores": {
                    "Anger": 0,
                    "Fear": 20,
                    "Joy": 5,
                    "Sadness": 30,
                    "Surprise": 0,
                    "Love": 15,
                    "Boredom": 0,
                    "Neutral": 30
                },
                "text": "Is she okay?"
            }
        },
        {
            "role": "participant_2",
            "content": {
                "emotion_scores": {
                    "Anger": 0,
                    "Fear": 30,
                    "Joy": 5,
                    "Sadness": 40,
                    "Surprise": 0,
                    "Love": 15,
                    "Boredom": 0,
                    "Neutral": 10
                },
                "text": "She's pregnant, and she's not sure what she wants to do."
